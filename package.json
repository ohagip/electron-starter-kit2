{
  "name": "AppName",
  "version": "0.0.1",
  "private": true,
  "main": "out/main/index.js",
  "scripts": {
    "clean": "rimraf out",
    "dev:main": "npm run build:main && electron .",
    "dev:renderer": "webpack serve --config src/renderer/webpack/dev.js",
    "build": "run-s build:*",
    "build:main": "tsc -p src/main",
    "build:renderer": "webpack --config src/renderer/webpack/prod.js --node-env production",
    "dist": "npm run clean && npm run build && electron-builder",
    "dist:win": "npm run clean && npm run build && electron-builder --win",
    "format": "npm run format:prettier && npm run format:es",
    "format:prettier": "prettier --write \"**/*.{js,ts,scss}\"",
    "format:es": "eslint --fix \"**/*.{js,ts}\"",
    "lint": "npm run lint:prettier && npm run lint:es",
    "lint:prettier": "prettier --check \"**/*.{js,ts,scss}\"",
    "lint:es": "eslint \"**/*.{js,ts}\""
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "glslify-loader": "^2.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-css-mqpacker": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.6.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "template-ejs-loader": "^0.9.3",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^0.8.0"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.1",
    "gsap": "^3.10.4",
    "tweakpane": "^3.0.8"
  }
}
